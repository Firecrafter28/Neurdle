{
    "name": "neurdle",
    "version": "1.1",
    "author": "Firecrafter28",
    "scripts": {
        "build-ts": "tsc",
        "build-ts-prod": "tsc --sourceMap false --declaration false --declarationMap false",
        "build-scss": "sass src/Styles",
        "build-scss-prod": "sass src/Styles --style compressed --no-source-map",
        "lint-ts": "eslint ./src",
        "lint-scss": "stylelint \"src/Styles/*.scss\""
    },
    "packageManager": "bun@1.2.21",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.41.0",
        "@typescript-eslint/parser": "^8.41.0",
        "bun": "^1.2.21",
        "eslint": "^9.34.0",
        "sass": "^1.91.0",
        "stylelint": "^16.23.1",
        "stylelint-config-standard-scss": "^15.0.1",
        "typescript": "^5.9.2"
    },
    "type": "module"
}
